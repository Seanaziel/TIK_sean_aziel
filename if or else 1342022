#include <iostream>
#include <string>
#include <cmath>
#include <cstdio>
using namespace std;

int main(){
    /* 
    jika nilai dengan sisa bagi = 0, bilangan genap
    jika bukan, bilangna ganjil
    */
    int nilai;
    cout<<"Masukkan Nilai = ";cin>>nilai;
    if (nilai % 2 == 0) {
        cout<<"Bilangan Genap";
    }
    else{
        cout<<"Bilangan Ganjil";
    }
    
}
