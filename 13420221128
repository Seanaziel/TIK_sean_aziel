#include <iostream>
#include <string>
#include <cmath>
#include <cstdio>
using namespace std;

     int main (){
int nilai;
  
 
    if (nilai % 2 == 0){
        cout << "Bilangan Genap";
        if (nilai < 10){
            cout << "Bilangan Genap kurang dari 10";
        }else {
	        cout << "bilangan genap lebih dari 10"; 
        }
    }else {
        cout << "Bilangan Ganjil";
        if (nilai <10){
            cout << "Bilangan Ganjil kurang dari 10";   
        }else{
            cout << "Bilangan Ganjil kurang dari 10"; 
        }
    }
}
