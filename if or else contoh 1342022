#include <iostream>
#include <string>
#include <cmath>
#include <cstdio>
using namespace std;

int main(){
    /* 
    jika nilai >=90 sangat memuaskan
    jika nilai >=80 memuaskan
    jika nilai >=75 cukup
    jika nilai <75 tidak lulus
    nilai tidak boleh  >100  or <0 keterangan diluar jangkauan.
    */
    
     int nilai;
     string keterangan;
    cout<<"Masukkan Nilai = ";cin>>nilai;
    
    if (nilai >100 || nilai<0) {
        keterangan = "diluar jangkauan";
        
    }else if (nilai >= 90) {
        keterangan = "sangat Memuaskan";
        
    }else if (nilai >= 80){
         keterangan = "memuaskan";
       
    }else{
        keterangan = "tidak lulus";
    }
    cout<<keterangan;
    
}
