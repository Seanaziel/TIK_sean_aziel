#include <iostream>
#include <string>
#include <cmath>
#include <cstdio>
using namespace std;

int main() {
     int angka[] = {};
     
     
     for(int j = 0; j <10 ; j ++){
        cout<<"Masukan angka = "; cin>>angka[j];
     }
     
     for(int i = 0; i<10 ; i ++){
        cout<<"Angka ke -  "<<i<< " = " <<angka[i]*angka[i]<<endl;
     }
    
    
   
    
    
    
    
    return 0;
}
