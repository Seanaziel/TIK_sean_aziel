#include <iostream>
#include <string>
#include <cmath>
#include <cstdio>
using namespace std;

int main() {
    int w[2][2] = {
        {7,1}, 
        {4,8},
    }; 
    int q[2][2] = {
       {5,2},
       {8,3},
    };
       
    for(int i=0; i<2; i++){
        for(int j=0; j<2; j++){
           cout<<w[i][j]+q[i][j]<<"  ";
           
    }
    }
    
    
    return 0;
}
